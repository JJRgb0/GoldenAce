{
  "builds": [
    {
      "src": "api/src/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/client/favicon.ico"
    },
    {
      "src": "/(assets|audio|games|icons|images|models|videos|api)/(.+)",
      "has": [
        {
          "type": "header",
          "key": "Sec-Fetch-Dest",
          "value": "document"
        }
      ],
      "dest": "/client/index.html",
      "status": 400
    },
    {
      "src": "/games/([^/]+)/Build/(.*\\.wasm)\\.br$",
      "headers": {
        "Content-Encoding": "br",
        "Content-Type": "application/wasm"
      },
      "dest": "/client/games/$1/Build/$2.br"
    },
    {
      "src": "/games/([^/]+)/Build/(.*\\.js)\\.br$",
      "headers": {
        "Content-Encoding": "br",
        "Content-Type": "application/javascript"
      },
      "dest": "/client/games/$1/Build/$2.br"
    },
    {
      "src": "/games/([^/]+)/Build/(.*\\.data)\\.br$",
      "headers": {
        "Content-Encoding": "br",
        "Content-Type": "application/octet-stream"
      },
      "dest": "/client/games/$1/Build/$2.br"
    },
    {
      "src": "/games/([^/]+)/Build/(.*)\\.br$",
      "headers": {
        "Content-Encoding": "br",
        "Content-Type": "application/octet-stream"
      },
      "dest": "/client/games/$1/Build/$2.br"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/src/server.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/client/assets/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/client/images/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/client/icons/$1"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/client/videos/$1"
    },
    {
      "src": "/audios/(.*)",
      "dest": "/client/audios/$1"
    },
    {
      "src": "/models/(.*)",
      "dest": "/client/models/$1"
    },
    {
      "src": "/games/(.*)",
      "dest": "/client/games/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ]
}